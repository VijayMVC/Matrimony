@model MvcShaadi.Models.Profile

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Profile</legend>

  

    <div class="display-label">Name</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Name)
    </div>

    <div class="display-label">Dob</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Dob)
    </div>

    <div class="display-label">MaritalStatus</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.MaritalStatus.Name)
    </div>

    <div class="display-label">State</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.State.Name)
    </div>

    <div class="display-label">Description</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Description)
    </div>

   
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.ProfileId }) |
